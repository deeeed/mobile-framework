{
  "name": "loggerdemo",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "NODE_ENV=development expo start --port 7761",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "NODE_ENV=development expo start --ios --port 7761",
    "web": "NODE_ENV=development expo start --web --port 7761",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "clean": "rimraf dist",
    "deploy": "NODE_ENV=production yarn clean && gh-pages -t -d dist --dest loggerdemo",
    "predeploy": "expo export -p web",
    "serve:static": "serve dist"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@gorhom/bottom-sheet": "^5.0.0-alpha.9",
    "@react-native-picker/picker": "2.7.5",
    "@react-navigation/drawer": "^6.6.15",
    "@siteed/react-native-logger": "0.7.1",
    "expo": "~51.0.21",
    "expo-constants": "~16.0.2",
    "expo-linking": "~6.3.1",
    "expo-localization": "~15.0.3",
    "expo-router": "~3.5.18",
    "expo-status-bar": "~1.12.1",
    "i18next": "^23.10.1",
    "intl-pluralrules": "^2.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.3",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-paper": "^5.12.3",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-vector-icons": "^10.0.3",
    "react-native-web": "^0.19.11"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/gh-pages": "^6",
    "@types/react": "~18.2.45",
    "eslint": "^8.57.0",
    "eslint-config-universe": "^12.0.0",
    "gh-pages": "^6.1.1",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
    "serve": "^14.2.3",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  },
  "private": true
